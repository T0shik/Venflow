{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/**.csproj" ],
          "src": "../../src/Venflow/Venflow"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
	  "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      },
      {
        "files": [
          "guides/**.md",
          "guides/**/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/samples/**",
		  "favicon.ico",
		  "venflow.svg"
        ]
      }
    ],
    "overwrite": [
      {
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "../",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "Venflow Documentation",
      "_appFooter": "<span>Copyright Â© 2021 Twenty</span>",
      "_enableSearch": "true",
	  "_appFaviconPath": "favicon.ico",
	  "_appLogoPath": "venflow.svg"
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/light-dark-theme"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
	"xref": [
	  "https://www.npgsql.org/xrefmap.yml"
	],
	"xrefService": [ 
	  "https://xref.docs.microsoft.com/query?uid={uid}" 
	],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
  }
}